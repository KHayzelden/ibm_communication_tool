    <script src="/socket.io/socket.io.js"></script>


</head>
<body>
    <div class="sidebar" data-color="blue">
        <div class="sidebar-wrapper">
            <div class="logo"> <!-- the users icon? -->
                <a class="simple-text">Welcome</a>
            </div>
            <ul class="nav">
                <li class="nav-item active active-pro">
                    <a class="nav-link active" href="./help">
                        <i class="nc-icon nc-puzzle-10"></i>
                        <p>Help</p>
                    </a>
                </li>
            </ul>
        </div>
    </div>
    <div class="wrapper">
        <div class="main-panel">
            <!-- Navbar -->
            <nav class="navbar navbar-expand-lg " color-on-scroll="500">
                <div class=" container-fluid  ">
                    <a class="navbar-brand"> <%=title%> </a>
                    <button href="" class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" aria-controls="navigation-index" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-bar burger-lines"></span>
                        <span class="navbar-toggler-bar burger-lines"></span>
                        <span class="navbar-toggler-bar burger-lines"></span>
                    </button>
                </div>
            </nav>
            <!-- End Navbar -->
            <div class="content">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="card text-center" style="max-width: 40rem;">
                                <div class="card-header">
                                    <h1><%=title%></h1>
                                </div>
                                <div class="card-body">
                                    <div id="login-register-div" class="row">
                                        <div id="login" class="col">
                                            <a href="/login" class="btn btn-outline-info"  style="background-color:#4091e2; border-color:#4091e2;color:#ffffff;">
                                                Sign In
                                            </a>
                                        </div>
                                        <div id="register" class="col">
                                            <a href="/register" class="btn btn-outline-info"  style="background-color:#4091e2; border-color:#4091e2;color:#ffffff;">
                                                Register
                                            </a>
                                        </div>
                                        <div id="guest" class="col-sm">
                                            <a href="/guest_login" class="btn btn-outline-info" style="background-color:#4091e2; border-color:#4091e2;color:#ffffff;">
                                                Continue as Guest
                                            </a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</body>
